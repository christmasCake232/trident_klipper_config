# SKR3 PINS Start

# FANx
# -FAN0: PB7, chamber fan
# -FAN1: PB6, chamber fan
# -FAN2: PB5, skirt fan

# HEx
# -BED: PD7, chamber lights
# -HE0: PB3, chamber heater
# -HE1: PB4, bed heater

# xM
# -XM: stepper x/b
# -YM: stepper y/a
# -YM: stepper y/a
# -ZAM: stepper z
# -E0M: stepper z
# -E1M: stepper z

# THx
# TB: PA1,  bed sensor
# TH0: PA2, chamber heater sensor
# TH1: PA3, chamber sensor

# SKR3 PINS End

[board_pins]
aliases:
# EXP1 header
    EXP1_1=PC5, EXP1_3=PB1, EXP1_5=PE9,  EXP1_7=PE11, EXP1_9=<GND>,
    EXP1_2=PB0, EXP1_4=PE8, EXP1_6=PE10, EXP1_8=PE12, EXP1_10=<5V>,
# EXP2 header
    EXP2_1=PA6, EXP2_3=PE7, EXP2_5=PB2, EXP2_7=PC4,   EXP2_9=<GND>,
    EXP2_2=PA5, EXP2_4=PA4, EXP2_6=PA7, EXP2_8=<RST>, EXP2_10=<NC>

[display]
lcd_type: uc1701
cs_pin: EXP1_3
a0_pin: EXP1_4
rst_pin: EXP1_5
encoder_pins: ^EXP2_5, ^EXP2_3
click_pin: ^!EXP1_2
contrast: 63
spi_software_miso_pin: EXP2_1
spi_software_mosi_pin: EXP2_6
spi_software_sclk_pin: EXP2_2

[mcu]
serial: /dev/serial/by-id/usb-Klipper_stm32h743xx_200022001051303232383230-if00
restart_method : command


[controller_fan left_fan]
heater : chamber_heater, heater_bed
pin : PB7 # FAN0
max_power: 1.0
shutdown_speed: 0.0
cycle_time: 0.010
hardware_pwm: False
kick_start_time: 2.0
off_below: 0.60
fan_speed: 1.0
idle_timeout: 600 # 10mins
idle_speed: 0.70

[controller_fan right_fan]
heater : chamber_heater, heater_bed
pin : PB6 # FAN1
max_power: 1.0
shutdown_speed: 0.0
cycle_time: 0.010
hardware_pwm: False
kick_start_time: 2.0
off_below: 0.60
fan_speed: 1.0
idle_timeout: 600 # 10mins
idle_speed: 0.70

[controller_fan skirt_fan]
stepper: stepper_x, stepper_y
pin : PB5 # FAN2
max_power: 1.0
shutdown_speed: 0.0
cycle_time: 0.010
hardware_pwm: False
kick_start_time: 1
off_below: 0.60
fan_speed: 1.0
idle_timeout: 600 # 10mins
idle_speed: 0.70

[heater_generic chamber_heater]
gcode_id : chamber_heater
heater_pin : PB3 # E0
sensor_pin : PA2 # TH0
max_power : 0.6
sensor_type : NTC 100K MGB18-104F39050L32
min_temp : -100
max_temp : 65

[verify_heater chamber_heater]
max_error: 120
check_gain_time: 60
hysteresis: 5
heating_gain: 2

[output_pin chamber_light]
pin : PD7 # BED
pwm : True
value : 0
shutdown_value : 0
cycle_time : 0.0078125
hardware_pwm: False

[output_pin _BEEPER_pin]
pin: EXP1_1
pwm: True
value: 0
shutdown_value: 0
cycle_time: 0.001

[temperature_sensor chamber_sensor]
gcode_id : chamber_sensor # left
sensor_type : ATC Semitec 104GT-2
sensor_pin : PA3 # TH1
min_temp : -100 #TODO
max_temp : 60

[heater_bed]
heater_pin: PB4 # E1
sensor_pin: PA1 # TB
sensor_type : NTC 100K MGB18-104F39050L32
max_power : 0.6
min_temp : -100
max_temp : 115

[verify_heater heater_bed]
max_error: 120
check_gain_time: 60
hysteresis: 5
heating_gain: 2

##  B Stepper - Left - XM
[stepper_x]
step_pin: PD4
dir_pin: PD3
enable_pin: !PD6
rotation_distance : 40
microsteps : 16
full_steps_per_rotation : 200
position_min : 0
position_endstop : 305
position_max : 305
homing_speed : 50   #Max 100
homing_retract_dist : 2
second_homing_speed: 1
homing_positive_dir : true
endstop_pin : ^ebb36:PC14

##  A Stepper - Right - YM
[stepper_y]
step_pin: PA15
dir_pin: PA8
enable_pin: !PD1
endstop_pin: ^PC3 # Y-STOP
rotation_distance : 40
microsteps : 16
full_steps_per_rotation : 200
position_min : 0
position_endstop : 300
position_max : 300
homing_speed : 50  #Max 100
homing_retract_dist : 5
second_homing_speed: 5
homing_positive_dir : true

# Z Stepper - FL - ZAM
[stepper_z]
step_pin: PE2
dir_pin: !PE3
enable_pin: !PE0
endstop_pin : probe:z_virtual_endstop
rotation_distance : 4
microsteps : 32
position_max : 200
position_min : -0.5
homing_speed : 3.0 # Leadscrews are slower than 2.4, 10 is a recommended max.
second_homing_speed : 1
homing_retract_dist : 2

# Z Stepper - BC - E0M
[stepper_z1]
step_pin: PD15
dir_pin: !PD14
enable_pin: !PC7
rotation_distance : 4
microsteps : 32

# Z Stepper - FR - E1M
[stepper_z2]
step_pin: PD11
dir_pin: !PD10
enable_pin: !PD13
rotation_distance : 4
microsteps : 32

[tmc5160 stepper_x]
cs_pin: PD5
spi_software_miso_pin: PE15
spi_software_mosi_pin: PE13
spi_software_sclk_pin: PE14
interpolate : True
run_current : 0.950
sense_resistor : 0.075
stealthchop_threshold : 0

[tmc5160 stepper_y]
cs_pin: PD0
spi_software_miso_pin: PE15
spi_software_mosi_pin: PE13
spi_software_sclk_pin: PE14
interpolate : True
run_current : 0.950
sense_resistor : 0.075
stealthchop_threshold : 0

[tmc2209 stepper_z]
uart_pin: PE1
interpolate : True
run_current : 0.500
hold_current : 0.400
stealthchop_threshold : 999999

[tmc2209 stepper_z1]
uart_pin: PC6
interpolate : True
run_current : 0.500
hold_current : 0.400
stealthchop_threshold : 999999

[tmc2209 stepper_z2]
uart_pin: PD12
interpolate : True
run_current : 0.500
hold_current : 0.400
stealthchop_threshold : 999999


[neopixel display_led]
#	To control Neopixel RGB in mini12864 display
pin: EXP1_6
chain_count: 3
initial_RED: 0.1
initial_GREEN: 0.5
initial_BLUE: 0.0
color_order: RGB


[temperature_sensor skr3]
sensor_type: temperature_mcu
min_temp: 0
max_temp: 80